{
  "name": "es6-expressjs",
  "version": "1.0.0",
  "description": "ES6 Nodejs express 4 starter",
  "main": "dist",
  "scripts": {
    "dev": "nodemon -w src --exec \"babel-node src --presets env,stage-0\"",
    "build": "babel src -s -D -d dist --presets env,stage-0",
    "start": "node dist",
    "prestart": "npm run -s build",
    "lint": "eslint 'src/**/*.js' --fix",
    "test": "eslint src"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1
    }
  },
  "author": "toan@tabvn.com",
  "license": "ISC",
  "dependencies": {
    "archiver": "^3.1.1",
    "aws-sdk": "^2.504.0",
    "body-parser": "^1.18.2",
    "cors": "^2.8.4",
    "dotenv": "^8.0.0",
    "eslint-plugin-prettier": "^3.1.0",
    "express": "^4.17.3",
    "lodash": "^4.17.4",
    "mongodb": "^2.2.33",
    "morgan": "^1.9.0",
    "multer": "^1.3.0",
    "multer-s3": "^2.9.0",
    "nodemailer": "^6.3.0",
    "prettier": "^1.18.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-promise": "^4.2.1",
    "nodemon": "^1.12.1"
  }
}
